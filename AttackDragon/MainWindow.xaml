<Window
    x:Class="AttackDragon.MainWindow"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:AttackDragon"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    Title="Attack Dragon"
    Loaded="Window_Loaded"
    WindowStartupLocation="CenterScreen"
    Width="1200" FontFamily="{StaticResource Fonts.FiraCode}"
    Height="600"
    Background="{StaticResource AttackDragon.SolidColorBrushes.MajestyDark}"
    MinWidth="800"
    MinHeight="450"
    mc:Ignorable="d">
    <Grid>
        <Frame x:Name="this_Frame"
               x:FieldModifier="private"
               NavigationUIVisibility="Hidden"
               JournalOwnership="UsesParentJournal"
               Navigated="Frame_Navigated"
               />
    </Grid>
</Window>
